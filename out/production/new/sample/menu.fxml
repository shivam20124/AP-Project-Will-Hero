<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="397.0" prefWidth="575.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.MenuScreen">
   <children>
      <ImageView fitHeight="400.0" fitWidth="634.0" layoutX="-5.0" layoutY="-3.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../20190110053934805.jpg" />
         </image>
      </ImageView>
      <Button fx:id="start_button" layoutX="61.0" layoutY="40.0" mnemonicParsing="false" onAction="#switch_to_play" prefHeight="25.0" prefWidth="68.0" style="-fx-background-color: blue;" text="Start" textFill="WHITE">
         <font>
            <Font name="SansSerif Regular" size="16.0" />
         </font></Button>
      <Button fx:id="load" layoutX="422.0" layoutY="14.0" mnemonicParsing="false" onAction="#load_game" prefHeight="25.0" prefWidth="102.0" style="-fx-background-color: orange;" text="Load Game" textFill="WHITE">
         <font>
            <Font name="Times New Roman" size="14.0" />
         </font></Button>
      <Button fx:id="high_score_button" layoutX="417.0" layoutY="57.0" mnemonicParsing="false" onAction="#View_HighScore" style="-fx-background-color: orange;" text="View High Score" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font></Button>
      <Button fx:id="exit_button" layoutX="14.0" layoutY="297.0" mnemonicParsing="false" onAction="#exit_game" style="-fx-background-color: Purple;" text="Exit" textFill="WHITE" textOverrun="CENTER_ELLIPSIS">
         <effect>
            <ColorAdjust />
         </effect>
         <font>
            <Font name="Serif Italic" size="18.0" />
         </font></Button>
   </children>
</AnchorPane>
